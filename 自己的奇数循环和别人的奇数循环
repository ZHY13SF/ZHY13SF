自己的
#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
int main() {
	int num = 0;
	int i = 0;
	while (num< 100)
	{
		num++;
		i++;
		if (1 == num % 2)
		{
			printf("奇数\n",("%d\n",num));
			printf("%d\n", i);
			
		}
}

	return 0;
}
别人的
int main() {
	int i = 0;
	for (i = 1; i <= 100; i++)
		if (1 == i % 2)
		{
			
			printf("%d\n", i);
		
		}





		return 0;
}
