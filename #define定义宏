#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>

#define Add(x,y)((x)+(y))
int x = 9;
int y = 10;



int main() 
{
	

	printf("%d\n", Add(x, y));
	return 0;


} 

  
  The following is another method .
  
  
  
#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>

#define Add(x,y)((x)+(y))



int main() 
{
	

	printf("%d\n", Add(5, 6));
	return 0;


} 
